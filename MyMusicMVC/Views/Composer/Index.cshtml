@model ListComposerViewModel

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var composer in Model.ListComposer)
        {
            <tr>
                <td class="text-center">@composer.FirstName </td>
                <td class="text-left">@composer.LastName </td>

            </tr>
        }
    </tbody>
</table>